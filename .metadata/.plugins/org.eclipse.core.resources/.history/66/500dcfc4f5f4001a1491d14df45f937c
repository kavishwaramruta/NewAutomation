package Array;

public class Pattern1 {

	public static void main(String[] args) 
	{
		String[][] arr = new String[5][2];

		// row1
		arr[0][0] = "1";
		arr[0][1] = "2";
		arr[0][2] = "3";
		arr[0][3] = "4";

		// row2

		arr[1][0] = "1";
		arr[1][1] = "2";
		arr[1][2] = "3";
		arr[1][3] = "4";

		// Row3

		arr[2][0] = "1";
		arr[2][1] = "2";
		arr[2][2] = "3";
		arr[2][3] = "4";

		// Row4

		arr[3][0] = "1";
		arr[3][1] = "2";
		arr[3][2] = "3";
		arr[3][3] = "4";

	

		for (int r = 0; r < 5; r++)

		{
			for (int c = 0; c < 2; c++)

			{
				System.out.print(" "+arr[r][c]);
			}
			System.out.println(" ");
		}
	}
}
		
	


