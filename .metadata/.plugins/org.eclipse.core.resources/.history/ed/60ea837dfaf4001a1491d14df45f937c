package Array;

public class pattern3 {

	public static void main(String[] args) {
String[][] arr = new String[4][4];
		

//initialization
		int i=0;
		do 
		{
			System.out.println(i);
		}
		//condition
	    while(i<17);
	    {
	    	//logic
	    	System.out.println(i);
	    	
	    	//increment
	    	i++;
	    }

	}

}
